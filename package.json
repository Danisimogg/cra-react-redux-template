{
  "name": "@npm/cra-react-redux-template",
  "private": true,
  "version": "1.0.0",
  "description": "Concept of a modular single page application",
  "main": "src/index.jsx",
  "scripts": {
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:eslint": "eslint --ext .js,.jsx .",
    "lint:prettier": "prettier '**/*.{js,jsx,json}' --write",
    "run-mock-server": "node ./utils/dev/run-mock-server.js",
    "generate-mock-data": "node ./utils/dev/generate-mock-data.js",
    "jest:e2e": "jest --config e2e-tests/jest.config.js --forceExit --detectOpenHandles"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ext .js,.jsx --fix  --max-warnings 0",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danisimogg/cra-react-redux-template.git"
  },
  "keywords": [
    "spa",
    "modular"
  ],
  "author": "daniall.husacov@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/Danisimogg/cra-react-redux-template#readme",
  "bugs": {
    "url": "https://github.com/Danisimogg/cra-react-redux-template/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@bem-react/classname": "^1.5.0",
    "browser-sync": "^2.26.12",
    "chalk": "^4.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "date-fns": "^1.30.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "formik": "^1.5.4",
    "fs": "0.0.1-security",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.14.1",
    "prompt": "^1.0.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^1.16.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-starter-kit": "^0.5.1",
    "redux-thunk": "^2.3.0",
    "replace": "^1.2.0",
    "rimraf": "^3.0.2",
    "semantic-ui-react": "^0.87.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "faker": "^4.1.0",
    "json-server": "^0.14.2",
    "prettier": "1.17.0"
  }
}
